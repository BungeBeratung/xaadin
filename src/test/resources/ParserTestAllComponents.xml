<?xml version="1.0" encoding="UTF-8"?>
<xaadin xmlns="http://xaadin.com/base/1.0.0"
				schemaLocation="http://xaadin.com/base/1.0.0 http://xaadin.com/xsd/xaadin-base-1.0.0.xsd">

	<horizontalLayout width="100%" spacing="true" columnExpandRatio="0.0, 1.0">
		<!-- Components -->
		<label id="label"/>
		<textField id="textField"/>
		<button id="button"/>
		<comboBox id="comboBox"/>
		<table id="table"/>
		<progressBar id="progressBar"/>
		<textArea id="textArea"/>
		<panel id="panel"/>
		<tabSheet id="tabSheet"/>
		<horizontalSplitPanel id="horizontalSplitPanel"/>
		<verticalSplitPanel id="verticalSplitPanel"/>
        <inlineDateField id="inlineDateField"/>
        <dateField id="dateField"/>

        <!-- Layouts -->
		<horizontalLayout id="horizontalLayout"/>
		<verticalLayout id="verticalLayout"/>
		<gridLayout id="gridLayout"/>
	</horizontalLayout>
</xaadin>
